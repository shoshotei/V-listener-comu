<div class="container px-5 px-sm-0">
 <div class="row">
   <div class="col-md-10 offset-md-1">
   <h2>ユーザー一覧</h2>
   <table class="table table-hover table-inverse">

     <thead>
       <tr>
         <th></th>
         <th>ユーザー名</th>
         <th></th>
       </tr>
     </thead>
     <tbody>
       <% @end_users.each do |end_user| %>
       <tr>
         <td><%= image_tag end_user.get_profile_image %></td>
         <td><%= end_user.name %></td>
         <td>
          <%= link_to "ユーザー詳細ページへ", admin_end_user_path(end_user.id) %>
         </td>
       </tr>
       <% end %>
     </tbody>
   </table>
  </div>
 </div>
</div>