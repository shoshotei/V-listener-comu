    <h2>プロフィール</h2>

       <%= image_tag end_user.get_profile_image %>

      <table class="table">
        <tbody>
          <tr></tr>
          <tr>
           <th>name</th>
           <th><%= end_user.name %></th>
          </tr>
          <tr>
            <th>introduction</th>
            <th><%= end_user.introduction %></th>
          </tr>
        </tbody>
      </table>

      <div class="row">
          <%= link_to "プロフィール編集", edit_end_user_path(end_user.id), class: "btn btn-outline-secondary btn-block" %>

      </div>

  <h2 class="mt-3">新規投稿</h2>
  <%= form_with model: submission do |f| %>

  <label for="image">画像選択</label></br>
  <%= f.file_field :image, accept: "image/*" %></br>

  <label for="opinion">本文</label></br>
  <%= f.text_area :body %></br>



  <%= f.submit '投稿', class:"btn btn-success btn-sm" %>

  <% end %>